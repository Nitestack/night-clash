{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@styles/*": ["src/styles/*"],
      "@constants/*": ["src/constants/*"],
      "@modules/*": ["src/modules/*"],
      "@database/*": ["src/configuration/Database/*"],
      "@models/*": ["src/configuration/Database/Models/*"],
      "@util/*": ["src/configuration/Util/*"],
      "@interfaces/*": ["src/configuration/Interfaces/*"],
      "@public/*": ["public/*"],
      "@images/*": ["public/Images/*"],
      "@actions/*": ["src/configuration/Actions/*"],
      "@pages/*": ["src/pages/*"],
      "@graphql/*": ["src/graphql/*"]
    },
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}
